void solve()
{
    ll n, x,flg=0;
    cin >> n >> x;
    vector<ll> v;
    for(int i = 0; i<n; i++)
    {
        ll a;
        cin >> a;
        if(x%a==0)
        {
            flg=1;
            v.pb(a);
        }
    }
    if(flg==0)
    {
        cout << -1 << endl;
        return;
    }
    ll lcm = v[0];
    for(int i = 1; i < v.size(); i++)
    {
        lcm = ( (abs(lcm*v[i])) / (__gcd(lcm,v[i])));
    }
    debug(lcm)
    if(lcm==x) cout << v.size() << endl;
    else cout << -1 << endl;
}

